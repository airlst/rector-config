{
    "name": "airlst/rector-config",
    "description": "Rector config for AirLST projects",
    "keywords": [
        "airlst",
        "rector",
        "rector-config"
    ],
    "homepage": "https://github.com/airlst/rector-config",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Orkhan Ahmadov",
            "email": "hey@orkhan.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "rector/rector": "^0.19.5"
    },
    "require-dev": {
        "airlst/php-cs-fixer-config": "^1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Airlst\\RectorConfig\\": "src"
        }
    },
    "scripts": {
        "cs-fixer": [
            "./vendor/bin/php-cs-fixer fix --verbose --config=.php-cs-fixer.php"
        ],
        "rector": [
            "./vendor/bin/rector"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
